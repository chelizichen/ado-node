{
  "name": "ado-node",
  "version": "1.8.0",
  "description": "ts-decorator for express server",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "repository": {
    "url": "https://github.com/chelizichen/ado-node",
    "type": "git"
  },
  "scripts": {
    "test": "ts-node-dev --project tsconfig.server.json test/server",
    "build": "tsc --project tsconfig.server.json && tsup index.ts --format cjs,esm && rm -r  dist/test/ && npm publish"
  },
  "keywords": [
    "express",
    "ts",
    "decorator",
    "ioc"
  ],
  "author": "chelizichen",
  "license": "ISC",
  "dependencies": {
    "@types/jsonwebtoken": "^8.5.9",
    "@types/mysql": "^2.15.21",
    "express": "^4.18.1",
    "jsonwebtoken": "^8.5.1",
    "mysql": "^2.18.1",
    "redis": "^4.3.1",
    "ts-node": "^10.9.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/node": "^18.6.3",
    "ts-node-dev": "^2.0.0",
    "tsup": "^6.2.3",
    "typescript": "^4.6.4"
  }
}
