{
  "name": "ado-node",
  "version": "2.5.1",
  "description": "ts-decorator for express server",
  "bin": {
    "ado": "bin/index.js",
    "ado-cli": "bin/dev.js",
    "hasaky": "bin/hasaky.js"
  },
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "repository": {
    "url": "https://github.com/chelizichen/ado-node",
    "type": "git"
  },
  "scripts": {
    "test": "ts-node-dev --project tsconfig.server.json test/server",
    "build": "tsc --project tsconfig.server.json && tsup index.ts --format cjs,esm && rm -r  dist/test/ && npm publish",
    "commit": "hasaky commit"
  },
  "keywords": [
    "express",
    "ts",
    "decorator",
    "ioc"
  ],
  "author": "chelizichen",
  "license": "ISC",
  "dependencies": {
    "@types/express-ws": "^3.0.1",
    "chalk": "^4.1.2",
    "commander": "^9.4.1",
    "ejs": "^3.1.8",
    "execa": "^6.1.0",
    "express": "^4.18.1",
    "express-ws": "^5.0.2",
    "fs-extra": "^11.1.0",
    "inquirer": "^7.1.4",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "multer": "^1.4.5-lts.1",
    "mysql": "^2.18.1",
    "redis": "^4.3.1",
    "reflect-metadata": "^0.1.13",
    "ts-node": "^10.9.1",
    "urllib": "^3.3.1"
  },
  "devDependencies": {
    "@types/execa": "^2.0.0",
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/lodash": "^4.14.186",
    "@types/multer": "^1.4.7",
    "@types/mysql": "^2.15.21",
    "@types/node": "^18.6.3",
    "ts-node-dev": "^2.0.0",
    "tsup": "^6.2.3",
    "typescript": "^4.6.4"
  }
}